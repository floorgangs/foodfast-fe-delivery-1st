{
  "expo": {
    "name": "FoodFast Customer",
    "slug": "foodfast-customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#EA5034"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.foodfast.customer",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#EA5034"
      },
      "package": "com.foodfast.customer",
      "permissions": [],
      "blockedPermissions": [
        "android.permission.DETECT_SCREEN_CAPTURE"
      ],
      "screenOrientation": "portrait"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
